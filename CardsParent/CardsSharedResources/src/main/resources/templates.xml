                    <url>wow.zamimg.com/hearthhead/sounds/VO_EX1_116_Play_01.ogg</url>
                </summon>
                <death>
                    <path>/sounds/Leeroy Jenkins/Death.wav</path>
                    <url>wow.zamimg.com/hearthhead/sounds/VO_EX1_116_Death_03.ogg</url>
                </death>
                <intro>
                    <path>/sounds/Leeroy Jenkins/Intro.wav</path>
                    <url>wow.zamimg.com/hearthhead/sounds/Pegasus_Stinger_Leeroy_Jenkins.ogg</url>
                </intro>
                <attack>
                    <path>/sounds/Leeroy Jenkins/Attack.wav</path>
                    <url>wow.zamimg.com/hearthhead/sounds/VO_EX1_116_Attack_02.ogg</url>
                </attack>
            </audio>
